{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-debugger": 1,
    "no-console": 0,
    "new-cap": 1,
    "strict": 1,
    "no-underscore-dangle": 1,
    "no-use-before-define": 1,
    "eol-last": 1,
    "quotes": [1, "single"],
    "jsx-quotes": [1, "prefer-single"],
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "no-undef": 1,
    "no-unused-vars": 1,
  }
}
